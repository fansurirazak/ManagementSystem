@model ManagementSystem.Models.tb_batches

@{
    ViewBag.Title = "Details";
}

<div class="container">
    <!-- DataTales Example -->
    <div class="card shadow mb-4">
        <div class="card-header py-3">

            <h6 class="m-0 font-weight-bold text-primary">Maklumat Batch</h6>
        </div>


        <div class="card-body">
            <div class="table-responsive">
                <h6 text-align="center">Butiran Maklumat</h6>

                <hr />
                <dl class="dl-horizontal">
                    <dt>
                        @Html.DisplayNameFor(model => model.ID)
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.ID)
                    </dd>
                    <dt>
                        @Html.DisplayNameFor(model => model.CreateDate)
                    </dt>
                    <dd>
                        @Html.DisplayFor(model => model.CreateDate)
                    </dd>

                </dl>
            </div>
            <p>

                <button class="btn btn-warning ">
                    @Html.ActionLink("Kemaskini", "Edit", new { id = Model.ID }, new { style = "color: white" })
                </button>

                <button class="btn btn-outline-primary">
                    @Html.ActionLink("Kembali", "Index", new { style = "color: white" })
                </button>

            </p>
        </div>
    </div>
</div>